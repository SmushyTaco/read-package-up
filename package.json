{
	"name": "@smushytaco/read-package-up",
	"version": "1.0.2",
	"description": "Read the closest package.json file",
	"license": "MIT",
	"private": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SmushyTaco/read-package-up.git"
	},
	"bugs": {
		"url": "https://github.com/SmushyTaco/read-package-up/issues"
	},
	"author": {
		"name": "SmushyTaco",
		"email": "personal@nikanradan.com"
	},
	"type": "module",
	"engines": {
		"node": ">=18.12.0",
		"npm": ">=8.19.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"clean": "npx del-cli dist",
		"build": "npm run clean && npx vite build",
		"prepare": "npm run build"
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"keywords": [
		"json",
		"read",
		"parse",
		"file",
		"fs",
		"graceful",
		"load",
		"package",
		"find",
		"up",
		"find-up",
		"findup",
		"look-up",
		"look",
		"search",
		"match",
		"resolve",
		"parent",
		"parents",
		"folder",
		"directory",
		"walk",
		"walking",
		"path"
	],
	"dependencies": {
		"@smushytaco/find-up-simple": "^1.0.0",
		"@smushytaco/read-pkg": "^1.0.1",
		"type-fest": "^4.31.0"
	},
	"devDependencies": {
		"@types/node": "^22.10.5",
		"del-cli": "^6.0.0",
		"@eslint/js": "^9.17.0",
		"@smushytaco/versionchecker": "^1.0.4",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"typescript-eslint": "^8.19.0",
		"vite": "^6.0.7",
		"vite-plugin-tsc-transpile": "^1.0.3",
		"vite-plugin-dts": "^4.4.0",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"typescript": "^5.7.2"
	}
}
